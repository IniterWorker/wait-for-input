{
    "poll": {
        "maxfd": 128,
        "timeout": -1
    },
    "gpios": [
        {
            "name": "btn_reset_network",
            "gpio_number": 22,
            "sh": "echo btn network reset",
            "edge": "falling",
            "direction": "in",
            "debounce": 200,
            "allow_already_exported": true
        },
        {
            "name": "btn_reboot",
            "gpio_number": 27,
            "sh": "echo reboot",
            "edge": "falling",
            "direction": "in",
            "debounce": 2000,
            "allow_already_exported": true
        }
    ]
}